{
    "key": "",
    "name": "Affiliate Deeplink",
    "description": "Gerador de Deeplink para Zanox, Afilio, Lomadee, Afiliados, Rakuten, Actionpay e Cityads",
    "version": "1.0.11",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": {"19": "link-open-flat.png", "38": "link-open-flat.png"},
        "default_title": "Affiliate Deeplink"
    },
    "background": {"persistent": false, "scripts": ["background.js"]},
    "options_page": "options.html",
    "web_accessible_resources": ["img/*"],
    "icons": {"16": "link-open-flat.png", "128": "link-open-flat.png"},
    "permissions": [
        "tabs",
        "contextMenus",
        "notifications",
        "storage",        
        "activeTab",
        "http://*.buscape.com/*",
        "http://*.zanox.com/*",
        "http://*.click.linksynergy.com/*",
        "http://*.afilio.com.br/*",
        "http://*.actionpay.net/*",
        "http://*.cityads.com/*"
    ]
}
